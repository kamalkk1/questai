{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "cleanUrls": true
        }
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": {
          "Content-Type": "text/javascript"
        }
      },
      {
        "src": "/_next/static/(.*)",
        "headers": {
          "Content-Type": "text/javascript"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "Content-Type": "text/html"
        }
      }
    ],
    "headers": [
      {
        "source": "/(.*).js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/javascript"
          }
        ]
      }
    ]
  }