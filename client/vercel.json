{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": {
          "Content-Type": "text/javascript"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "Content-Type": "text/html"
        }
      }
    ],
    "trailingSlash": false,
    "cleanUrls": true
  }