{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "headers": [
        {
          "source": "/assets/(.*.js)",
          "headers": [{"key": "Content-Type", "value": "application/javascript"}]
        }
      ],
      "routes": [
        { "src": "/assets/.*", "dest": "/assets/[src]" },
        { "src": ".*", "dest": "/index.html" }
      ]
  }
  